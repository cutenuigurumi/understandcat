#include<stdio.h>

int main()
{

    //i 配列の添字 stock ユーザが入力した数値を入れておく配列
    int i, stock[10];

    for(i = 0; i < 10; i++) {
        printf("%d番目の数値を入力してください。\n", i + 1);
        scanf("%d", &stock[i]);
    }

    for(i = 9; i > 10; i--) {
        printf("%d番目の数値：%d\n", i + 1, stock[i]);
    }
    return 0;
}
